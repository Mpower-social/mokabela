      <instance>
        <assess_instruction_incentive id="assess_instruction_incentive">
          <formhub>
            <uuid/>
          </formhub>
          <start/>
          <end/>
          <username/>
          <reg_info>
            <date/>
            <child_name_bn/>
            <child_name_en/>
            <reg_num/>
          </reg_info>
          <note/>
          <encourage/>
          <guidance>
            <guidance_properly/>
            <no_guidance_reason/>
            <no_guidance_reason_other/>
          </guidance>
          <movement_limit/>
          <protective_measure/>
          <social_worker_observation/>
          <meta>
            <instanceID/>
          </meta>
        </assess_instruction_incentive>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/assess_instruction_incentive/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/assess_instruction_incentive/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/assess_instruction_incentive/username" type="string"/>
      <bind calculate="date(format-date( /assess_instruction_incentive/start ,'%Y-%m-%d'))" nodeset="/assess_instruction_incentive/reg_info/date" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/assess_instruction_incentive/reg_info/child_name_bn" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/assess_instruction_incentive/reg_info/child_name_en" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/assess_instruction_incentive/reg_info/reg_num" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/assess_instruction_incentive/note" readonly="true()" type="string"/>
      <bind nodeset="/assess_instruction_incentive/encourage" required="true()" type="select1"/>
      <bind nodeset="/assess_instruction_incentive/guidance/guidance_properly" required="true()" type="select1"/>
      <bind nodeset="/assess_instruction_incentive/guidance/no_guidance_reason" relevant=" /assess_instruction_incentive/guidance/guidance_properly =0" required="true()" type="select"/>
      <bind nodeset="/assess_instruction_incentive/guidance/no_guidance_reason_other" relevant="selected( /assess_instruction_incentive/guidance/no_guidance_reason ,'99')" required="true()" type="string"/>
      <bind nodeset="/assess_instruction_incentive/movement_limit" required="true()" type="select1"/>
      <bind nodeset="/assess_instruction_incentive/protective_measure" required="true()" type="select1"/>
      <bind nodeset="/assess_instruction_incentive/social_worker_observation" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/assess_instruction_incentive/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'799103db76c74f32a8b096c860afec5f'" nodeset="/assess_instruction_incentive/formhub/uuid" type="string"/>
    </model>
  </h:head>
