      </itext>
      <instance>
        <case_transfer id="case_transfer">
          <formhub>
            <uuid/>
          </formhub>
          <start/>
          <end/>
          <username/>
          <reg_info>
            <date/>
            <child_name_bn/>
            <child_name_en/>
            <reg_num/>
            <case_type/>
            <case_id/>
          </reg_info>
          <transfer_date/>
          <transfer_reason/>
          <transfer_reason_other/>
          <transfer_place/>
          <office_type/>
          <office_name/>
          <Comments/>
          <meta>
            <instanceID/>
          </meta>
        </case_transfer>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/case_transfer/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/case_transfer/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/case_transfer/username" type="string"/>
      <bind calculate="date(format-date( /case_transfer/start ,'%Y-%m-%d'))" nodeset="/case_transfer/reg_info/date" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/case_transfer/reg_info/child_name_bn" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/case_transfer/reg_info/child_name_en" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/case_transfer/reg_info/reg_num" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/case_transfer/reg_info/case_type" required="true()" type="string"/>
      <bind nodeset="/case_transfer/reg_info/case_id" required="true()" type="string"/>
      <bind nodeset="/case_transfer/transfer_date" required="true()" type="date"/>
      <bind nodeset="/case_transfer/transfer_reason" required="true()" type="select1"/>
      <bind nodeset="/case_transfer/transfer_reason_other" relevant="selected( /case_transfer/transfer_reason ,'99')" required="true()" type="string"/>
      <bind nodeset="/case_transfer/transfer_place" required="true()" type="select1"/>
      <bind nodeset="/case_transfer/office_type" required="true()" type="select1"/>
      <bind nodeset="/case_transfer/office_name" required="true()" type="select1"/>
      <bind nodeset="/case_transfer/Comments" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/case_transfer/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'3cfbdd8dd8344283b6daabbbb14a28eb'" nodeset="/case_transfer/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="field-list" ref="/case_transfer/reg_info">
      <input ref="/case_transfer/reg_info/date">
        <label ref="jr:itext('/case_transfer/reg_info/date:label')"/>
      </input>
      <input appearance="db" ref="/case_transfer/reg_info/child_name_bn">
        <label ref="jr:itext('/case_transfer/reg_info/child_name_bn:label')"/>
      </input>
      <input appearance="db" ref="/case_transfer/reg_info/child_name_en">
        <label ref="jr:itext('/case_transfer/reg_info/child_name_en:label')"/>
      </input>
      <input appearance="db" ref="/case_transfer/reg_info/reg_num">
        <label ref="jr:itext('/case_transfer/reg_info/reg_num:label')"/>
      </input>
      <input ref="/case_transfer/reg_info/case_type">
        <label ref="jr:itext('/case_transfer/reg_info/case_type:label')"/>
      </input>
      <input ref="/case_transfer/reg_info/case_id">
        <label ref="jr:itext('/case_transfer/reg_info/case_id:label')"/>
      </input>
    </group>
    <input ref="/case_transfer/transfer_date">
      <label ref="jr:itext('/case_transfer/transfer_date:label')"/>
    </input>
    <select1 ref="/case_transfer/transfer_reason">
      <label ref="jr:itext('/case_transfer/transfer_reason:label')"/>
      <item>
        <label ref="jr:itext('/case_transfer/transfer_reason/1:label')"/>
        <value>1</value>
      </item>
      <item>
        <label ref="jr:itext('/case_transfer/transfer_reason/2:label')"/>
        <value>2</value>
      </item>
      <item>
        <label ref="jr:itext('/case_transfer/transfer_reason/3:label')"/>
        <value>3</value>
      </item>
      <item>
        <label ref="jr:itext('/case_transfer/transfer_reason/99:label')"/>
        <value>99</value>
      </item>
    </select1>
    <input ref="/case_transfer/transfer_reason_other">
      <label ref="jr:itext('/case_transfer/transfer_reason_other:label')"/>
    </input>
    <select1 ref="/case_transfer/transfer_place">
      <label ref="jr:itext('/case_transfer/transfer_place:label')"/>
      <item>
        <label ref="jr:itext('/case_transfer/transfer_place/1:label')"/>
        <value>1</value>
      </item>
      <item>
        <label ref="jr:itext('/case_transfer/transfer_place/2:label')"/>
        <value>2</value>
      </item>
    </select1>
    <select1 ref="/case_transfer/office_type">
      <label ref="jr:itext('/case_transfer/office_type:label')"/>
      <item>
        <label ref="jr:itext('/case_transfer/office_type/1:label')"/>
        <value>1</value>
      </item>
    </select1>
    <select1 ref="/case_transfer/office_name">
      <label ref="jr:itext('/case_transfer/office_name:label')"/>
      <item>
        <label ref="jr:itext('/case_transfer/office_name/1:label')"/>
        <value>1</value>
      </item>
    </select1>
    <input ref="/case_transfer/Comments">
      <label ref="jr:itext('/case_transfer/Comments:label')"/>
    </input>
  </h:body>
