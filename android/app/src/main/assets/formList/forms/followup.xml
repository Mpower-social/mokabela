      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/followup/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/followup/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/followup/username" type="string"/>
      <bind calculate="date(format-date( /followup/start ,'%Y-%m-%d'))" nodeset="/followup/reg_info/date" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/followup/reg_info/child_name_bn" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/followup/reg_info/child_name_en" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/followup/reg_info/reg_num" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/followup/support_info" required="true()"/>
      <bind nodeset="/followup/support_info/support" required="false()" type="select1"/>
      <bind nodeset="/followup/support_info/support_date" required="false()" type="date"/>
      <bind nodeset="/followup/support_info/support_authorities" required="false()" type="string"/>
      <bind nodeset="/followup/measures_taken" required="true()" type="select1"/>
      <bind nodeset="/followup/measures_taken_other" relevant="selected( /followup/measures_taken ,'99')" type="string"/>
      <bind nodeset="/followup/baby_progress" required="true()" type="string"/>
      <bind nodeset="/followup/other_information" required="false()" type="string"/>
      <bind nodeset="/followup/child_view" required="true()" type="string"/>
      <bind nodeset="/followup/guardian_comment" required="true()" type="string"/>
      <bind nodeset="/followup/score" required="false()" type="int"/>
      <bind nodeset="/followup/po_comment" required="true()" type="string"/>
      <bind nodeset="/followup/recommendation" required="true()" type="string"/>
      <bind nodeset="/followup/child_sign" required="false()" type="binary"/>
      <bind nodeset="/followup/guardian_sign" required="false()" type="binary"/>
      <bind nodeset="/followup/next_followup_date" required="false()" type="date"/>
      <bind nodeset="/followup/po_sign" required="false()" type="binary"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/followup/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'df136ed0129d46609530ec5b3c4e716e'" nodeset="/followup/formhub/uuid" type="string"/>
