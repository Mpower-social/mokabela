      <bind calculate="date(format-date( /intervention_plan/start ,'%Y-%m-%d'))" nodeset="/intervention_plan/reg_info/date" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/intervention_plan/reg_info/child_name_bn" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/intervention_plan/reg_info/child_name_en" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/intervention_plan/reg_info/reg_num" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/intervention_plan/plan" required="true()" type="select"/>
      <bind nodeset="/intervention_plan/plan_other" relevant="selected( /intervention_plan/plan ,'99')" required="true()" type="string"/>
      <bind nodeset="/intervention_plan/office_type" required="true()" type="select1"/>
      <bind nodeset="/intervention_plan/office_name" required="true()" type="select1"/>
      <bind nodeset="/intervention_plan/comments" required="false()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/intervention_plan/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'8e3c98385efd455890f84770bf099c4a'" nodeset="/intervention/formhub/uuid" type="string"/>
    </model>
