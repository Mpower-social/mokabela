      <instance>
        <assess_extended_family id="assess_extended_family">
          <formhub>
            <uuid/>
          </formhub>
          <start/>
          <end/>
          <username/>
          <reg_info>
            <date/>
            <child_name_bn/>
            <child_name_en/>
            <reg_num/>
          </reg_info>
          <e_family>
            <program_participation/>
            <assist_in_program/>
            <child_care/>
            <attachment/>
            <secure_attachment/>
          </e_family>
          <social_worker_observation/>
          <meta>
            <instanceID/>
          </meta>
        </assess_extended_family>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/assess_extended_family/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/assess_extended_family/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/assess_extended_family/username" type="string"/>
      <bind calculate="date(format-date( /assess_extended_family/start ,'%Y-%m-%d'))" nodeset="/assess_extended_family/reg_info/date" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/assess_extended_family/reg_info/child_name_bn" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/assess_extended_family/reg_info/child_name_en" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/assess_extended_family/reg_info/reg_num" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/assess_extended_family/e_family" required="true()"/>
      <bind nodeset="/assess_extended_family/e_family/program_participation" required="true()" type="select1"/>
      <bind nodeset="/assess_extended_family/e_family/assist_in_program" required="true()" type="select1"/>
      <bind nodeset="/assess_extended_family/e_family/child_care" required="true()" type="select1"/>
      <bind nodeset="/assess_extended_family/e_family/attachment" required="true()" type="select1"/>
      <bind nodeset="/assess_extended_family/e_family/secure_attachment" required="true()" type="select1"/>
      <bind nodeset="/assess_extended_family/social_worker_observation" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/assess_extended_family/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'3657abe9d9734b39892da475735e1cd3'" nodeset="/assess_extended_family/formhub/uuid" type="string"/>
    </model>
