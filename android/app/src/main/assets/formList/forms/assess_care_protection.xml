      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/assess_care_protection/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/assess_care_protection/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/assess_care_protection/username" type="string"/>
      <bind calculate="date(format-date( /assess_care_protection/start ,'%Y-%m-%d'))" nodeset="/assess_care_protection/reg_info/date" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/assess_care_protection/reg_info/child_name_bn" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/assess_care_protection/reg_info/child_name_en" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/assess_care_protection/reg_info/reg_num" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/assess_care_protection/note1" readonly="true()" type="string"/>
      <bind nodeset="/assess_care_protection/diet/balanced_diet" required="true()" type="select1"/>
      <bind nodeset="/assess_care_protection/diet/no_balanced_diet_reason" relevant=" /assess_care_protection/diet/balanced_diet =0" required="true()" type="select"/>
      <bind nodeset="/assess_care_protection/diet/no_balanced_diet_reason_other" relevant="selected( /assess_care_protection/diet/no_balanced_diet_reason ,'99')" required="true()" type="string"/>
      <bind nodeset="/assess_care_protection/accomodation/standard_accommodation" required="true()" type="select1"/>
      <bind nodeset="/assess_care_protection/accomodation/no_standard_accommodation_reason" relevant="selected( /assess_care_protection/accomodation/standard_accommodation ,'0')" required="true()" type="select"/>
      <bind nodeset="/assess_care_protection/accomodation/no_standard_accommodation_reason_other" relevant="selected( /assess_care_protection/accomodation/no_standard_accommodation_reason ,'99')" required="true()" type="string"/>
      <bind nodeset="/assess_care_protection/clean_cloth" required="true()" type="select1"/>
      <bind nodeset="/assess_care_protection/social_worker_observation" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/assess_care_protection/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'2eeb765e487c41ffa2c13b311952bb54'" nodeset="/assess_care_protection/formhub/uuid" type="string"/>
    </model>
