      <bind calculate="date(format-date( /intervention_action/start ,'%Y-%m-%d'))" nodeset="/intervention_action/reg_info/date" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/intervention_action/reg_info/child_name_bn" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/intervention_action/reg_info/child_name_en" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/intervention_action/reg_info/reg_num" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/intervention_action/action" required="true()" type="select"/>
      <bind nodeset="/intervention_action/action_other" relevant="selected( /intervention_action/action ,'99')" required="true()" type="string"/>
      <bind nodeset="/intervention_action/implementer" required="true()" type="string"/>
      <bind nodeset="/intervention_action/implementation_date" required="true()" type="date"/>
      <bind nodeset="/intervention_action/comments" required="false()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/intervention_action/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'8328bb0949b34903834506d3e8934761'" nodeset="/intervention_action/formhub/uuid" type="string"/>
