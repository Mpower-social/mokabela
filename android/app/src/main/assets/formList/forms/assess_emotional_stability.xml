      <bind calculate="concat('uuid:', uuid())" nodeset="/assess_emotional_stability/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'8f6c153133564ccebad11351885b4cef'" nodeset="/assess_emotional_stability/formhub/uuid" type="string"/>
