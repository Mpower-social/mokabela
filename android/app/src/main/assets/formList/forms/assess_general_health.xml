      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/assess_general_health/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/assess_general_health/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/assess_general_health/username" type="string"/>
      <bind nodeset="/assess_general_health/reg_info/child_name_bn" required="true()" type="string"/>
      <bind nodeset="/assess_general_health/reg_info/child_name_en" required="true()" type="string"/>
      <bind nodeset="/assess_general_health/reg_info/reg_num" required="true()" type="string"/>
      <bind calculate="date(format-date( /assess_general_health/start ,'%Y-%m-%d'))" nodeset="/assess_general_health/date" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/assess_general_health/note1" readonly="true()" type="string"/>
      <bind nodeset="/assess_general_health/treatment/child_visits_doctor_lasty_one_year" required="true()" type="select1"/>
      <bind nodeset="/assess_general_health/treatment/chronic_illness" required="true()" type="select1"/>
      <bind nodeset="/assess_general_health/treatment/illness" relevant=" /assess_general_health/treatment/chronic_illness =1" required="true()" type="select"/>
      <bind nodeset="/assess_general_health/treatment/teatment" relevant=" /assess_general_health/treatment/chronic_illness =1" required="true()" type="select1"/>
      <bind nodeset="/assess_general_health/treatment/no_treatment_reason" relevant=" /assess_general_health/treatment/chronic_illness =0 or  /assess_general_health/treatment/teatment =2" required="true()" type="select"/>
      <bind nodeset="/assess_general_health/treatment/no_treatment_reason_other" relevant="selected( /assess_general_health/treatment/no_treatment_reason ,'99')" required="true()" type="string"/>
      <bind nodeset="/assess_general_health/vaccine/vaccination" required="true()" type="select1"/>
      <bind nodeset="/assess_general_health/vaccine/current_age_vaccine" relevant=" /assess_general_health/vaccine/vaccination =0" required="true()" type="select"/>
      <bind nodeset="/assess_general_health/vaccine/vaccination_provided_other" relevant="selected( /assess_general_health/vaccine/current_age_vaccine ,'99')" required="true()" type="string"/>
      <bind nodeset="/assess_general_health/social_worker_observation" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/assess_general_health/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'ee8ab08e38a7433a8623399cf017989d'" nodeset="/assess_general_health/formhub/uuid" type="string"/>
    </model>
