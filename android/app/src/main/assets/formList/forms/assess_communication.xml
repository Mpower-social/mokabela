      <instance>
        <assess_communication id="assess_communication">
          <formhub>
            <uuid/>
          </formhub>
          <start/>
          <end/>
          <username/>
          <reg_info>
            <child_name_bn/>
            <child_name_en/>
            <reg_num/>
          </reg_info>
          <date/>
          <note1/>
          <communication>
            <express_emotion_in_face/>
            <understand_gesture/>
            <listen_with_attention/>
            <respond_enthusiastically/>
            <hesitate_seek_help/>
            <hesitate_reason/>
            <hesitate_reason_other/>
          </communication>
          <social_worker_observation/>
          <meta>
            <instanceID/>
          </meta>
        </assess_communication>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/assess_communication/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/assess_communication/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/assess_communication/username" type="string"/>
      <bind nodeset="/assess_communication/reg_info/child_name_bn" required="true()" type="string"/>
      <bind nodeset="/assess_communication/reg_info/child_name_en" required="true()" type="string"/>
      <bind nodeset="/assess_communication/reg_info/reg_num" required="true()" type="string"/>
      <bind calculate="date(format-date( /assess_communication/start ,'%Y-%m-%d'))" nodeset="/assess_communication/date" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/assess_communication/note1" readonly="true()" type="string"/>
      <bind nodeset="/assess_communication/communication/express_emotion_in_face" required="true()" type="select1"/>
      <bind nodeset="/assess_communication/communication/understand_gesture" required="true()" type="select1"/>
      <bind nodeset="/assess_communication/communication/listen_with_attention" required="true()" type="select1"/>
      <bind nodeset="/assess_communication/communication/respond_enthusiastically" required="true()" type="select1"/>
      <bind nodeset="/assess_communication/communication/hesitate_seek_help" required="true()" type="select1"/>
      <bind nodeset="/assess_communication/communication/hesitate_reason" relevant=" /assess_communication/communication/hesitate_seek_help =1" required="true()" type="select"/>
      <bind nodeset="/assess_communication/communication/hesitate_reason_other" relevant="selected( /assess_communication/communication/hesitate_reason ,'99')" required="true()" type="string"/>
      <bind nodeset="/assess_communication/social_worker_observation" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/assess_communication/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'3181d068eb2142c0a6a3cb7e9d33236b'" nodeset="/assess_communication/formhub/uuid" type="string"/>
    </model>
  </h:head>
